import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns

url = "https://raw.githubusercontent.com/datasciencedojo/datasets/master/titanic.csv"
df = pd.read_csv(url)

# Histogram for 'Age'
plt.hist(df['Age'].dropna(), bins=20, edgecolor='black')
plt.xlabel("Age")
plt.ylabel("Frequency")
plt.show()
